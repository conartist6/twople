{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2017", "es2018"],
    "noEmit": true,
    "declaration": false,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "pretty": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true
  }
}
